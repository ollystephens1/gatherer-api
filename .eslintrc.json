{
  "extends": "airbnb-base",
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "no-param-reassign": "off",
    "comma-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "no-confusing-arrow": "off",
    "arrow-body-style": "off",
    "no-irregular-whitespace": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack.config.js"
      }
    }
  }
}